new entryCh, lookup(`rho:registry:lookup`), stdout(`rho:io:stdout`) in {
    lookup!(`REGISTRY_URI`, *entryCh) |
    for(entry <- entryCh) {
      entry!(
        {
          "type": "ADD",
          "payload": {
            "id": "FILE_ID",
            "file": "DAPPY_FILE",
            "nonce": "NONCE",
            "signature": "SIGNATURE"
          }
        },
        *stdout
      )
    }
  }